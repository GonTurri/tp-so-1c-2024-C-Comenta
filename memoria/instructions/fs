RESIZE 128
SET AX 0
SET CX 4
SET DX 3
SET EDX 12
SET EAX 5000000
MOV_OUT AX EAX
IO_FS_CREATE Int5 mati.txt 
IO_FS_TRUNCATE Int5 mati.txt EDX
IO_FS_WRITE Int5 mati.txt AX CX DX
IO_FS_CREATE Int5 uri.txt
IO_FS_CREATE Int5 santi.txt
IO_FS_TRUNCATE Int5 santi.txt EDX
IO_FS_WRITE Int5 santi.txt AX CX DX
IO_FS_TRUNCATE Int5 uri.txt EDX
IO_FS_READ Int5 santi.txt AX CX DX
MOV_IN EAX AX
IO_FS_READ Int5 mati.txt AX CX DX
MOV_IN EAX AX
IO_FS_READ Int5 uri.txt AX CX DX
MOV_IN EAX AX
EXIT


SET AX 10

SET DX 12
IO_FS_WRITE Int5 mati.txt AX CX DX
SET DX 20
IO_FS_WRITE Int5 mati.txt AX CX DX


SET DX 0
IO_FS_WRITE Int5 mati.txt AX CX DX
SET DX 5
IO_FS_WRITE Int5 mati.txt AX CX DX
SET DX 10
IO_FS_WRITE Int5 mati.txt AX CX DX
IO_FS_TRUNCATE Int5 uri.txt BX

MOV_IN EBX AX